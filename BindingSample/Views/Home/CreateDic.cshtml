@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center mb-5">
    <h1 class="display-4">Create</h1>
</div>


<div class="container">
    <form asp-controller="Home" asp-action="CreateDicResult" method="get" class="form row">

        <h4 class="col-12 text-center mb-3">Tags</h4>
        <div class="form-group col-4">
            <label class="form-label">Key: Name</label>
            <input type="text" name="pr.tags[Name]" class="form-control" />
        </div>
        <div class="form-group col-4">
            <label class="form-label">Key: Title</label>
            <input type="text" name="pr.tags['Title']" class="form-control" />
        </div>
        <div class="form-group col-4">
            <label class="form-label">Key: Number</label>
            <input type="number" name="pr.tags[Number]" class="form-control" />
        </div>
        <div class="form-group col-12 row mt-5">
            <input type="submit" name="Submit" class="form-control btn btn-success" />
        </div>

    </form>

</div>
